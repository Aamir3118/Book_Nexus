<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.SubscriptionEndFragment"
    android:background="#181A1A"
    >

    <RelativeLayout
        android:id="@+id/frame_131"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginHorizontal="16dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="81dp"
        >
        <TextView
            android:id="@+id/premium_plan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/premium_pla"
            android:textAppearance="@style/premium_plan"
            android:lineSpacingExtra="-3sp"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/ulimited_in"
            android:layout_below="@id/premium_plan"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ulimited_in"
            android:textAppearance="@style/ulimited_in"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_below="@id/frame_131"
        android:id="@+id/frame_129"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginHorizontal="16dp"
        >
        <RelativeLayout
            android:id="@+id/frame_127"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/premium_ind"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Premium Individual"
                android:textSize="16sp"
                android:textColor="#FFFFFF"
                />
            <RelativeLayout
                android:id="@+id/frame_120"
                android:layout_below="@id/premium_ind"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/premium_banner"
                >
                <RelativeLayout
                    android:id="@+id/frame_121"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="8dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginBottom="20dp"
                    >
                    <TextView
                        android:id="@+id/monthly"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Monthly"
                        android:textSize="14sp"
                        android:textColor="#EAF4F4"
                        android:gravity="center_horizontal|center_vertical"
                        />
                    <TextView
                        android:id="@+id/offer_expire"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/monthly"
                        android:layout_marginTop="5dp"
                        android:text="Offer Expires"
                        android:textSize="18sp"
                        android:textColor="#FFFFFF"
                        android:gravity="center_horizontal|center_vertical"
                        />
                </RelativeLayout>
                <TextView
                    android:layout_below="@id/frame_121"
                    android:id="@+id/your_premiu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/your_premiu"
                    android:paddingHorizontal="20dp"
                    android:textAppearance="@style/billed_recu"
                    android:gravity="center_horizontal|center_vertical"
                    />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="20dp"
            android:layout_below="@id/frame_127"
            android:id="@+id/frame_128"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <RelativeLayout
                android:id="@+id/button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_continue_layout"
                >
                <TextView
                    android:id="@+id/button_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/button"
                    android:textAppearance="@style/button"
                    android:layout_centerVertical="true"
                    android:textStyle="bold"
                    android:layout_centerHorizontal="true"
                    />
            </RelativeLayout>
            <TextView
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/button"
                android:id="@+id/no_thanks"
                android:text="NO THANKS"
                android:textColor="#EAF4F4"
                android:textSize="14sp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@id/frame_128"
            android:id="@+id/frame_134"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginHorizontal="15dp"
            >
            <TextView
                android:id="@+id/your_member"
                android:layout_width="358dp"
                android:layout_height="83dp"
                android:text="@string/your_member"
                android:textAppearance="@style/by_continue"
                android:lineSpacingExtra="2sp"
                android:gravity="center_vertical"
                />
            <TextView
                android:id="@+id/by_continue"
                android:layout_below="@id/your_member"
                android:layout_width="358dp"
                android:layout_height="33dp"
                android:text="@string/by_continue"
                android:textAppearance="@style/by_continue"
                android:lineSpacingExtra="2sp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>